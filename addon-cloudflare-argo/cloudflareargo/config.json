{
	"name": "Cloudflare Argo",
	"version": "2.0",
	"slug": "argocloudflare",
	"description": "Argo for Home Assistant",
	"url": "https://github.com/wlatic/hassio.addons/blob/master/cloudflareargo/readme.md",
	"ingress": false,
	"panel_icon": "mdi:docker",
	"homeassistant": "0.104.0",
	"arch": [
    "aarch64",
    "amd64",
    "armhf",
    "armv7"
  ],
	"map": [
         "config:rw"
  ],

	"startup": "services",
	"boot": "auto",
	"options": {
		
			"certificate": "/config/cert/argo.pem",
			"hostname": "host",
			"service": "http://hassio:8123",
			"hostname2": "null",
			"service2": "null",
			"hostname3": "null",
			"service3": "null",
			"add-config": "null"
	},
	 "schema": {
    "certificate": "str",
    "hostname": "str",
    "service": "str",
    "hostname2": "str",
    "service2": "str",
    "hostname3": "str",
    "service3": "str",
    "add-config": "str"
  }
}
